{
  "name": "nativescript-web-adapter",
  "version": "0.1.6",
  "description": "Web adapter for NativeScript applications",
  "type": "module",
  "main": "./dist/nativescript-web-adapter.umd.js",
  "module": "./dist/nativescript-web-adapter.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/nativescript-web-adapter.es.js",
      "require": "./dist/nativescript-web-adapter.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "create:web": "node tools/create-web-platform.cjs",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "ns-web": "./tools/cli.cjs"
  },
  "files": [
    "dist",
    "tools",
    "templates",
    "core",
    "angular"
  ],
  "dependencies": {
    "commander": "^11.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@angular/common": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "rxjs": "^7.8.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.12"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "@angular/core": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/router": "^18.0.0",
    "rxjs": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "@angular/core": {
      "optional": true
    },
    "@angular/common": {
      "optional": true
    },
    "@angular/platform-browser": {
      "optional": true
    },
    "@angular/router": {
      "optional": true
    },
    "rxjs": {
      "optional": true
    }
  }
}
